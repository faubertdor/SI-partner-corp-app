<div class="row">
    <%= render partial: 'shared/errors', locals: { model: @general_info } %>
</div>

<div class="row">
  <%= form_for(@general_info) do |f| %>
    <p>
      <div class="col-md-4">
        <%= f.label :corporate_name %><br>
        <%= f.text_field :corporate_name %>
      </div>
      <div class="col-md-4">
        <%= f.label :trade_name %><br>
        <%= f.text_field :trade_name %>
      </div>
      <div class="col-md-4">
        <%= f.label :Country_of_Incorporation %><br>
        <%= f.text_field :country_of_inc %>
      </div>
    </p>

    <p>
      <div class="col-md-4">
        <%= f.label :street_address %>(Not PO Box)<br>
        <%= f.text_field :street_address %>
      </div>
      <div class="col-md-4">
        <%= f.label :city %><br>
        <%= f.text_field :city %>
      </div>
      <div class="col-md-4">
        <%= f.label :state %><br>
        <%= f.text_field :state %>
      </div>
    </p>

    <p>
      <div class="col-md-4">
        <%= f.label :zip_code %><br>
        <%= f.text_field :zip_code %>
      </div>
      <div class="col-md-4">
        <%= f.label :country %><br>
        <%= f.text_field :country %>
      </div>
      <div class="col-md-4">
        <%= f.label :landline_number %><br>
        <%= f.text_field :landline_number %>
      </div>
    </p>

    <p>
      <div class="col-md-4">
        <%= f.label :cell_number %><br>
        <%= f.text_field :cell_number %>
      </div>

      <div class="col-md-4">
        <%= f.label :email %><br>
        <%= f.text_field :email %>
      </div>
      <div class="col-md-4">
        <%= f.label :state_of_Incorporation %><br>
        <%= f.text_field :state_of_inc %>
      </div>
    </p>

    <p>
      <div class="col-md-4">
        <%= f.label :US_tax_id %><br>
        <%= f.text_field :tax_id %>
      </div>
      <div class="col-md-4">
        <%= f.label :nature_of_business %><br>
        <%= f.text_field :nature_of_business %>
      </div>
      <div class="col-md-4">
        <%= f.label :website %><br>
        <%= f.text_field :website %>
      </div>
    </p>

    <p>
      <div class="col-md-6">
        <% if !@general_info.new_record? %>
          <%= link_to 'Back', @general_info, class: "btn btn-primary" %>
        <% end %>
      </div>
      <div class="col-md-6">
        <%= f.submit class: "button expand", class: "btn btn-primary" %>
      </div>
    </p>
  <% end %>
</div>
  