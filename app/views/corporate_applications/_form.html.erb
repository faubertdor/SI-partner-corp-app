<div>
  <%= form_for(@banking_info, url: corporate_applications_submit_path) do |f| %>
    <div class="field">
      <%= f.label :bank_name %><br>
      <%= f.text_field :bank_name %>
    </div>
    
    <div class="field">
      <%= f.label :account_manager %><br>
      <%= f.text_field :acc_manager %>
    </div>
    
    <div class="field">
      <%= f.label :street_address %><br>
      <%= f.text_field :street_address %>
    </div>
    
    <div class="field">
      <%= f.label :city %><br>
      <%= f.text_field :city %>
    </div>
    
    <div class="field">
      <%= f.label :state %><br>
      <%= f.text_field :state %>
    </div>
    
    <div class="field">
      <%= f.label :country %><br>
      <%= f.text_field :country %>
    </div>
    
    <div class="field">
      <%= f.label :zip_code %><br>
      <%= f.text_field :zip_code %>
    </div>
    
    <div class="actions">
      <%= f.submit "Submit Application" %>
    </div>
  <% end %>
</div>